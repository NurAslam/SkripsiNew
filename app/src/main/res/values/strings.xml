    <string name="hijaiyah">HIJAIYAH</string>
    <string name="hijaiyah1">huruf hijaiyah yaitu abjad arab yang dimulai dari (alif) sampai dengan (ya).</string>
    <string name="harokat">HAROKAT</string>
    <string name="harokat1">harokat atau tasykil adalah tanda baca atau diakritik yang ditempatkan pada huruf arab untuk memperjelas gerakan dan pengucapan huruf tersebut.</string>
    <string name="tanwin">TANWIN</string>
    <string name="tanwin1">tanwin adalah tanda baca/diakritik/harakat pada tulisan arab untuk menyatakan bahwa huruf pada akhir kata tersebut diucapkan layaknya bertemu dengan huruf nun mati.</string>
    <string name="ikro">IQRO</string>
    <string name="click">Click</string>
    <string name="ikro1">iqra biasanya diterjemahkan dengan \'bacalah\',merupakan kata pertama dari wahyu yang disampaikan Allah SWT kepada Nabi Muhammad SAW .</string>
    <string name="fathah">FATHAH</string>
    <string name="fathah1">harokat fathah adalah harakat yang berbentuk seperti garis horizontal yang diletakkan diatas suatu huruf hijaiyah untuk melambangkan fonem \'A\'.</string>
    <string name="kasroh">KASROH</string>
    <string name="kasroh1">harokat kasroh adalah harokat yang berbentuk sepeti garis horizontal yang diletakkan dibawah suatu huruf hijaiyah untuk melambangkan fonem \'I\'.</string>
    <string name="dhomah">DHOMAH</string>
    <string name="dhomah1">harokat dhomah adalah harokat yang berbentuk wawu kecil yang diletakkan diatas suatu huruf hijaiyah untuk melambangkan fonem U\'.</string>
    <string name="fathah_tain">FATHAH TAIN</string>
    <string name="fathah_tain1">fathah tain dibaca dengan \'AN\'.</string>
    <string name="kasroh_tain">KASROH TAIN</string>
    <string name="kasroh_tain1">kasroh tain dibaca dengan \'IN\'.</string>
    <string name="dhomah_tain">DHOMAH TAIN</string>
    <string name="dhomah_tain1">domah tain dibaca dengan \'UN\'.</string>
    <string name="iqro_1">IQRO 1</string>
    <string name="iqro_1_1">bergaris miring diatas = fathah, dibaca \'A\'.</string>
    <string name="iqro_2">IQRO 2</string>
    <string name="iqro_2_1">huruf sambung boleh dibaca putus.</string>
    <string name="iqro_3">IQRO 3</string>
    <string name="iqro_3_1">huruf miring bergaris bawah = kasroh, dibaca \'I\'.</string>
